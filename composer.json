{
  "name": "estev/service",
  "type": "project",
  "version": "1.0.0",
  "description": "",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "nyholm/psr7": "^1.8",
    "php-amqplib/php-amqplib": "^v3.2.0",
    "psr/container": "^2.0",
    "psr/log": "^3.0",
    "symfony/console": "^7.1",
    "symfony/uid": "^7.1",
    "symfony/yaml": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "core/",
      "Project\\": "project/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Composer\\Script\\ClassMapGenerator::generate"
    ]
  },
  "extra": {
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "qossmic/deptrac": "^2.0"
  }
}
